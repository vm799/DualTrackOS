{
  "name": "dualtrack-os",
  "version": "1.0.0",
  "description": "The operating system for perimenopausal women running dual careers",
  "private": true,
  "dependencies": {
    "@sentry/react": "^10.32.1",
    "@stripe/stripe-js": "^8.6.0",
    "@supabase/supabase-js": "^2.87.1",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.11.0",
    "react-scripts": "5.0.1",
    "zustand": "^5.0.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=true npm run lint && GENERATE_SOURCEMAP=true react-scripts build",
    "build:skip-validation": "GENERATE_SOURCEMAP=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run build && vercel --prod",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "validate": "npm run lint",
    "validate:hooks": "node scripts/validate-hooks.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest-environment-jsdom": "^30.2.0"
  }
}
