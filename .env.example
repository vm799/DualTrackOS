# Supabase Configuration
# Get these from: https://app.supabase.com/project/YOUR_PROJECT/settings/api
REACT_APP_SUPABASE_URL=your-project-url.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# Stripe Configuration
# Get your publishable key from: https://dashboard.stripe.com/apikeys
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_YOUR_PUBLISHABLE_KEY_HERE

# Stripe Price IDs
# Create products and prices at: https://dashboard.stripe.com/products
# After creating, copy the price IDs (they start with 'price_')
# See docs/STRIPE_SETUP_GUIDE.md for detailed instructions

# Starter Tier ($4.99/month or $49/year)
REACT_APP_STRIPE_STARTER_MONTHLY=price_XXXXXXXXXXXXXXXXXXXXXXXX
REACT_APP_STRIPE_STARTER_ANNUAL=price_XXXXXXXXXXXXXXXXXXXXXXXX

# Premium Tier ($9.99/month or $99/year)
REACT_APP_STRIPE_PREMIUM_MONTHLY=price_XXXXXXXXXXXXXXXXXXXXXXXX
REACT_APP_STRIPE_PREMIUM_ANNUAL=price_XXXXXXXXXXXXXXXXXXXXXXXX

# Gold Tier ($19.99/month or $199/year)
REACT_APP_STRIPE_GOLD_MONTHLY=price_XXXXXXXXXXXXXXXXXXXXXXXX
REACT_APP_STRIPE_GOLD_ANNUAL=price_XXXXXXXXXXXXXXXXXXXXXXXX
